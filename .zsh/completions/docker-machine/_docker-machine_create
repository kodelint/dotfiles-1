#compdef docker-machine

# zsh completions for 'docker-machine'
# automatically generated with http://github.com/RobSis/zsh-completion-generator
local arguments

arguments=(
    {--driver,-d}'["											Driver to create machine with.]'
    '--engine-install-url[//get.docker.com"							Custom URL to use for engine installation \[$MACHINE_DOCKER_INSTALL_URL\]]'
    '--engine-opt[option --engine-opt option\]						Specify arbitrary flags to include with the created engine in the form flag=value]'
    '--engine-insecure-registry[option --engine-insecure-registry option\]	Specify insecure registries to allow with the created engine]'
    '--engine-registry-mirror[option --engine-registry-mirror option\]		Specify registry mirrors to use]'
    '--engine-label[option --engine-label option\]						Specify labels for the created engine]'
    '--engine-storage-driver[Specify a storage driver to use with the engine]'
    '--engine-env[option --engine-env option\]						Specify environment variables to set in the engine]'
    '--swarm[Configure Machine with Swarm]'
    '--swarm-image["										Specify Docker image to use for Swarm \[$MACHINE_SWARM_IMAGE\]]'
    '--swarm-master[Configure Machine to be a Swarm master]'
    '--swarm-discovery[Discovery service to use with Swarm]'
    '--swarm-strategy["										Define a default scheduling strategy for Swarm]'
    '--swarm-opt[option --swarm-opt option\]							Define arbitrary flags for swarm]'
    '--swarm-host[//0.0.0.0:3376"									ip/socket to listen on for Swarm master]'
    '--swarm-addr[addr to advertise for Swarm (default: detect and use the machine IP)]'
    '--tls-san[option --tls-san option\]							Support extra SANs for TLS certs]'
    '*:filename:_files'
)

_arguments -s $arguments
